{
    "name": "sabbottlabs/laravel-adminerneo",
    "description": "Laravel integration for AdminerNeo database management",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "adminerneo",
        "database",
        "management",
        "mysql",
        "pgsql",
        "sqlite",
        "mssql"
    ],
    "license": ["Apache-2.0", "GPL-2.0-only"],
    "authors": [
        {
            "name": "Stephen Abbott",
            "email": "sabbott@sabbottlabs.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "ext-pdo": "*",
        "sabbottlabs/laravel-adminerneo-builder": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "SabbottLabs\\AdminerNeo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SabbottLabs\\AdminerNeo\\AdminerNeoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {}
}